{
  "presets": ["react"],
  "plugins": [
    "transform-object-rest-spread",
    "lodash",
    "babel-plugin-transform-decorators-legacy",
    "transform-es2015-classes"
  ],
  "env": {
  	"production": {
  		"plugins": [
  			"babel-plugin-transform-remove-debugger"
  		]
  	},
    "development": {
      "presets": ["react-hmre"]
    },
    "test": {
      "plugins": [
        ["webpack-loaders", { "config": "webpack.config.node.js", "verbose": false }]
      ]
    }
  }
}
